#app secret
WECHAT_SERVER_SECRET=SECRET
#appId
WECHAT_SERVER_APPID=APPID
#TOKEN
WECHAT_SERVER_TOKEN=TOKEN
#获取ACCESS_TOKEN的URL
WECHAT_ACCESS_TOKEN_URL=https\://api.weixin.qq.com/cgi-bin/token?grant_type\=client_credential&secret\=APPSECRET&appid\=APPID
